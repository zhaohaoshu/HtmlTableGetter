{
    "manifest_version": 2,
    "name": "HtmlTableGetter",
    "version": "1.0",
    "description": "Get HTML table.",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "HtmlTableGetter@local"
        }
    }
}
